{
  "name": "@icehunter/material-gallery",
  "version": "1.0.0",
  "description": "Material Gallery Components",
  "main": "lib/index.js",
  "module": "lib/esm/index.js",
  "typings": "lib/esm/index.d.ts",
  "author": "Ryan Wilson <syndicated.life@gmail.com> (https://syndicated.life/)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Icehunter/material-gallery.git"
  },
  "keywords": [
    "material",
    "gallery",
    "react"
  ],
  "bugs": {
    "url": "https://github.com/Icehunter/material-gallery/issues"
  },
  "dependencies": {
    "@charlietango/use-native-lazy-loading": "^1.9.0",
    "@material-ui/core": "^4.11.4",
    "@material-ui/icons": "^4.11.2",
    "react-intersection-observer": "^8.32.0"
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "scripts": {
    "clean": "rimraf build lib",
    "start": "react-scripts start",
    "build:app": "npm run clean && react-scripts build",
    "build": "npm run clean && just build",
    "lint": "tsc --noEmit && eslint --cache 'src/**/*.{js,ts,tsx}'",
    "prepare": "husky install",
    "storybook": "start-storybook -p 6006 -s public --no-manager-cache",
    "build-storybook": "build-storybook -s public",
    "test": "react-scripts test"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@icehunter/eslint-config": "^1.1.1",
    "@storybook/addon-actions": "^6.3.1",
    "@storybook/addon-essentials": "^6.3.1",
    "@storybook/addon-links": "^6.3.1",
    "@storybook/node-logger": "^6.3.1",
    "@storybook/preset-create-react-app": "^3.1.7",
    "@storybook/react": "^6.3.1",
    "@storybook/theming": "^6.3.1",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/user-event": "^13.1.9",
    "@types/jest": "^26.0.15",
    "@types/node": "^15.12.5",
    "@types/react": "^17.0.11",
    "@types/react-dom": "^17.0.8",
    "clsx": "^1.1.1",
    "css-loader": "^5.2.6",
    "husky": "^6.0.0",
    "just-scripts": "^1.5.4",
    "lint-staged": "^11.0.0",
    "node-sass": "^4.14.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "4.0.3",
    "sass-loader": "^10.0.0",
    "storybook-addon-performance": "^0.16.0",
    "style-loader": "^2.0.0",
    "typescript": "^4.3.4",
    "web-vitals": "^2.0.1"
  },
  "lint-staged": {
    "**/*.{ts,tsx}": [
      "eslint --cache --fix",
      "prettier --write"
    ]
  },
  "eslintConfig": {
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  }
}
